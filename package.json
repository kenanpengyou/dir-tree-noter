{
  "name": "dir-tree-noter",
  "version": "1.0.0",
  "description": "Generate proper directory tree codes for you.",
  "scripts": {
    "start": "node server",
    "build": "webpack --config webpack.production.config.js -p",
    "release": "npm run build & http-server -p 8000",
    "clean-deploy": "rm -rf public",
    "pre-deploy": "git clone https://github.com/kenanpengyou/dir-tree-noter.git --branch gh-pages public",
    "deploy": "npm run build & cd public & git add --all && git commit -m 'update gh-pages with deploy script' & git push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kenanpengyou/dir-tree-noter.git"
  },
  "keywords": [
    "directory",
    "tree"
  ],
  "author": "Liang Zhu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kenanpengyou/dir-tree-noter/issues"
  },
  "homepage": "https://github.com/kenanpengyou/dir-tree-noter#readme",
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "clean-webpack-plugin": "^0.1.9",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "eslint": "^2.13.1",
    "eslint-plugin-react": "^5.2.2",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "node-sass": "^3.13.0",
    "open-browser-webpack-plugin": "0.0.2",
    "postcss-loader": "^0.9.1",
    "react-hot-loader": "^1.3.1",
    "resolve-url-loader": "^1.6.0",
    "sass-loader": "^3.2.3",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "react": "^15.4.1",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.6",
    "redux": "^3.6.0"
  }
}
